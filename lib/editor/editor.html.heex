<div>
  <%= for block <- @page.blocks do %>
    <%= content_tag(
      block.type,
      [
        contenteditable: true,
        data: [block_id: block.id],
        id: "content-#{block.id}",
        phx_hook: "ContentEditable",
        phx_target: @myself
      ],
      do: raw(block.content)
    ) %>
  <% end %>
</div>
